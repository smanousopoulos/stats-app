{
  "name": "stats-frontend",
  "version": "1.0.0",
  "description": "A simple application to display trivia questions and statistics",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --env.env=dev",
    "serve:dev": "webpack-dev-server --env.env=dev --hot",
    "build:prod": "npm run build -- -p --env.env=prod",
    "serve:prod": "npm run build:prod && node serve.js",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "start": "npm run serve:prod",
    "test": "jest .",
    "coverage": "jest --coverage",
    "webpack-defaults": "webpack-defaults"
  },
  "author": "Stelios Manousopoulos",
  "repository": {
    "type": "git",
    "url": "https://github.com/smanousopoulos/stats-app.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.6.0",
    "mockdate": "^2.0.2",
    "node-sass": "^4.7.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^6.5.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.3.2"
  }
}
